@{
    ViewBag.Title = "ViewSnaps";
}
@using CryptoDataWebScraper.Models
@model List<CryptoDataWebScraper.Models.Ledger>




<p>SnapShotDisplay</p>

@{
    foreach (var ledger in Model)
    {
        <tr>
            <td>@ledger.Time</td>
            <td>@ledger.LedgerId</td>
            <td> <a href="/ViewSnap/@ledger.LedgerId">See Your Snap !</a></td>
        </tr>
    }
}
